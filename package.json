{
  "name": "koa2-cors",
  "version": "2.0.6",
  "description": "cors middleware for koa2",
  "main": "dist/index.js",
  "files": [
    "src/index.js",
    "dist/index.js"
  ],
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "test:cover": "NODE_ENV=test istanbul cover _mocha",
    "build": "NODE_ENV=production rm -rf dist & babel src -d dist",
    "lint": "eslint --ext .js src --format codeframe",
    "ci": "npm run lint && npm run test:cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zadzbw/koa2-cors.git"
  },
  "keywords": [
    "koa2",
    "cors",
    "middleware"
  ],
  "author": "zad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zadzbw/koa2-cors/issues"
  },
  "homepage": "https://github.com/zadzbw/koa2-cors#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "chai": "^4.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "istanbul": "^1.1.0-alpha.1",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
